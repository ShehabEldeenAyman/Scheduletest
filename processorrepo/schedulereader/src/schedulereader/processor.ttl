@prefix rdfc: <https://w3id.org/rdf-connect#>.
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#>.
@prefix sh: <http://www.w3.org/ns/shacl#>.
@prefix xsd: <http://www.w3.org/2001/XMLSchema#>.

rdfc:schedulereader rdfc:pyImplementationOf rdfc:Processor;
    rdfs:label "schedulereader";
    rdfs:comment "Template processor to kickstart the development of your next Python RDF-Connect processor.";
    rdfc:modulePath "schedulereader.processor";
    rdfc:class "schedulereader".

[ ] a sh:NodeShape;
    sh:targetClass rdfc:schedulereader;
    sh:property  [
        sh:class rdfc:Writer;
        sh:path rdfc:writer;
        sh:name "writer";
        sh:minCount 0;
        sh:maxCount 1;
    ].
